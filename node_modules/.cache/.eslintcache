e);\n    const { state } = this;\n    if (!keys.find((key) => state[key] === \"\")) return true;\n    return false;\n  };\n\n  onSubmitButtonClick = async (event) => {\n    event.preventDefault();\n    const { name, email, image, description } = this.state;\n    const upDatedUser = { name, email, image, description };\n    this.setState({ loading: true });\n    await updateUser(upDatedUser);\n    this.setState({ profileEditDone: true });\n  };\n  render() {\n    const {\n      name,\n      email,\n      description,\n      image,\n      loading,\n      isButtonDisabled,\n      profileEditDone,\n    } = this.state;\n    return (\n      <div data-testid=\"page-profile-edit\">\n        {profileEditDone && <Redirect to=\"/profile\" />}\n        <Header />\n        <div className=\"form-box\">\n          <h5 className=\"form-step\"> </h5>\n          <form>\n            <div className=\"field1\">\n              <section>\n                <img\n                  src={image}\n                  alt=\"imagem do usuario\"\n                  className=\"profile-image\"\n                />\n                <input\n                  data-testid=\"edit-input-image\"\n                  name=\"image\"\n                  className=\"image-input\"\n                  id=\"imageInput\"\n                  type=\"text\"\n                  placeholder=\"put the image url here\"\n                  onChange={this.onInputChange}\n                />\n              </section>\n              <input name=\"name\" value={ name } onChange={this.onInputChange} placeholder=\"Name\" />\n              <input placeholder=\"Phone 000-000-0000\" />\n              <input\n                id=\"email-input\"\n                value={email}\n                name=\"email\"\n                onChange={this.onInputChange}\n                placeholder=\"E-mail\"\n              />\n              <textarea\n                id=\"description-input\"\n                \n                type=\"text\"\n                onChange={this.onInputChange}\n                placeholder=\"description\"\n              />\n            </div>\n\n            <button\n              disabled={isButtonDisabled}\n              className=\"nextBtn\"\n              type=\"submit\"\n              id=\"submitBtn\"\n            >\n              {\" \"}\n              submit\n            </button>\n          </form>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default ProfileEdit;\n","/home/humberto/Desktop/ProjectsGitPortfolio/trybeTunesProject/src/pages/NotFound.jsx",[],"/home/humberto/Desktop/ProjectsGitPortfolio/trybeTunesProject/src/services/musicsAPI.js",[],"/home/humberto/Desktop/ProjectsGitPortfolio/trybeTunesProject/src/services/userAPI.js",[],"/home/humberto/Desktop/ProjectsGitPortfolio/trybeTunesProject/src/services/favoriteSongsAPI.js",[],"/home/humberto/Desktop/ProjectsGitPortfolio/trybeTunesProject/src/services/searchAlbumsAPI.js",[],"/home/humberto/Desktop/ProjectsGitPortfolio/trybeTunesProject/src/Components/Loading.jsx",[],"/home/humberto/Desktop/ProjectsGitPortfolio/trybeTunesProject/src/Components/Header.jsx",[],"/home/humberto/Desktop/ProjectsGitPortfolio/trybeTunesProject/src/Components/MusicCard.jsx",[],"/home/humberto/Desktop/ProjectsGitPortfolio/trybeTunesProject/src/Components/AlbumThumbnail.jsx",[],{"ruleId":"142","replacedBy":"143"},{"ruleId":"144","replacedBy":"145"},{"ruleId":"146","severity":1,"message":"147","line":4,"column":8,"nodeType":"148","messageId":"149","endLine":4,"endColumn":15},{"ruleId":"146","severity":1,"message":"150","line":78,"column":7,"nodeType":"148","messageId":"149","endLine":78,"endColumn":18},{"ruleId":"146","severity":1,"message":"151","line":80,"column":7,"nodeType":"148","messageId":"149","endLine":80,"endColumn":14},"no-native-reassign",["152"],"no-negated-in-lhs",["153"],"no-unused-vars","'Loading' is defined but never used.","Identifier","unusedVar","'description' is assigned a value but never used.","'loading' is assigned a value but n